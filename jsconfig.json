{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2022",
    "checkJs": false,
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["*.js", "scrapers/**/*.js", "public/**/*.js"],
  "exclude": ["node_modules"]
}
